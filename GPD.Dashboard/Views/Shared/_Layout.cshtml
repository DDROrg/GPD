<!DOCTYPE html>
<html>
<head>
    @{
        var up = SessionManager.GetInstance().GetUserProfile();
        var isAdmin = SessionManager.GetInstance().AdminRole();
    }
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="~/Content/Circloid/images/required/ico/favicon.ico">
    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Content/Circloid/images/required/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Content/Circloid/images/required/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/Content/Circloid/images/required/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="~/Content/Circloid/images/required/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="~/Content/Circloid/images/required/ico/favicon.png">

    <title>@ViewBag.Title</title>

    <!-- Required CSS Files -->
    <link type="text/css" href="~/Content/Circloid/css/required/bootstrap/bootstrap.min.css" rel="stylesheet">
    @*<link href='http://fonts.googleapis.com/css?family=Roboto:400,300&subset=latin,latin-ext' rel='stylesheet' type='text/css'>*@
    <link type="text/css" href="~/Content/Circloid/js/required/jquery-ui-1.11.0.custom/jquery-ui.min.css" rel="stylesheet" />
    <link type="text/css" href="~/Content/Circloid/js/required/jquery-ui-1.11.0.custom/jquery-ui.structure.min.css" rel="stylesheet" />
    <link type="text/css" href="~/Content/Circloid/js/required/jquery-ui-1.11.0.custom/jquery-ui.theme.min.css" rel="stylesheet" />
    <link type="text/css" href="~/Content/Circloid/css/required/mCustomScrollbar/jquery.mCustomScrollbar.min.css" rel="stylesheet" />
    <link type="text/css" href="~/Content/Circloid/css/required/icheck/all.css" rel="stylesheet" />
    <link type="text/css" href="~/Content/Circloid/fonts/metrize-icons/styles-metrize-icons.css" rel="stylesheet">
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">

    <!-- Optional CSS Files -->
    <!-- add CSS files here -->
    @Styles.Render("~/bundles/angular/css")
    @Styles.Render("~/bundles/custom/css")
    <!-- More Required CSS Files -->
    <link type="text/css" href="~/Content/Circloid/css/styles-core.css" rel="stylesheet" />
    <link type="text/css" href="~/Content/Circloid/css/styles-core-responsive.css" rel="stylesheet" />

    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="~/Content/Circloid/js/required/misc/ie10-viewport-bug-workaround.js"></script>

    <!--[if IE 7]>
    <link type="text/css" href="~/Content/Circloid/css/required/misc/style-ie7.css" rel="stylesheet">
    <script type="text/javascript" src="~/Content/Circloid/fonts/lte-ie7.js"></script>
    <![endif]-->
    <!--[if IE 8]>
    <link type="text/css" href="~/Content/Circloid/css/required/misc/style-ie8.css" rel="stylesheet">
    <![endif]-->
    <!--[if lte IE 8]>
    <script type="text/javascript" src="~/Content/Circloid/css/required/misc/excanvas.min.js"></script>
    <![endif]-->
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <script type="text/javascript">
        var __RootUrl = '@string.Format("{0}://{1}{2}", Request.Url.Scheme, Request.Url.Authority, Url.Content("~"))';
        var __UserEmail = '@(up == null ? "": up.Email)';
    </script>
</head>
<body data-ng-app="GPD">
    <div class="container-fluid">
        <div id="header-container">
            <div class="header-bar navbar navbar-inverse" role="navigation">
                <!-- NOTE TO READER: Accepts the following class(es) "navbar-fixed-top" class -->
                <div class="container">
                    <div class="navbar-header">
                        <div class="logo">
                            <a href="~/Home/Index">
                                <img class="default-logo" src="~/Content/Circloid/images/required/logo-default.png" width="156" height="44" alt="Logo">
                                <img class="small-logo" src="~/Content/Circloid/images/required/logo-small.png" width="48" height="44" alt="Logo">
                            </a>
                        </div>
                        <div class="header-info">
                            <!-- START Header User Profile -->
                            <div class="header-profile">
                                <!-- NOTE TO READER: Accepts the following class(es) "animate" class -->
                                <ul class="header-profile-menu">
                                    <li class="">
                                        @if (Request.IsAuthenticated)
                                        {
                                            <form action="/Account/Logout" id="logoutForm" method="post">
                                                @Html.AntiForgeryToken()
                                                <ul class="nav navbar-nav navbar-right">
                                                    <li><a href="javascript:document.getElementById('logoutForm').submit()">Log Off: @up.FirstName</a></li>
                                                </ul>
                                            </form>
                                        }
                                    </li>
                                </ul>
                            </div>
                            <!-- END Header User Profile -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END Header Container -->
        <!-- START Body Container -->
        <div id="body-container">
            <div id="left-column" class="">
                @RenderSection("leftmenu", required: false)                
            </div>
            <div id="right-column">
                <div class="right-column-content">@RenderBody()</div>
                <div id="footer-container">
                    <div class="footer-content"><p>&copy; @DateTime.Now.Year - GPD Analytics</p></div>
                </div>
            </div>
        </div>
    </div>
    <!-- Required JS Files -->
    <script type="text/javascript" src="~/Content/Circloid/js/required/jquery-3.2.1.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/required/jquery-ui-1.11.0.custom/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/required/bootstrap/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/required/jquery.easing.1.3-min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/required/jquery.mCustomScrollbar.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/required/misc/jquery.mousewheel-3.0.6.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/required/misc/retina.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/required/icheck.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/required/misc/jquery.ui.touch-punch.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/required/circloid-functions.js"></script>

    <!-- Optional JS Files -->
    <!-- Optional JS Files -->
    <script type="text/javascript" src="~/Content/Circloid/js/optional/flot/jquery.flot.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/flot/jquery.flot.JUMlib.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/flot/jquery.flot.resize.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/flot/jquery.flot.tooltip.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/flot/jquery.flot.pie.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/flot/jquery.flot.stack.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/flot/jquery.flot.bubbles.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/flot/jquery.flot.time.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/flot/jquery.flot.orderBars.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/easy-pie-chart/jquery.easypiechart.min.js"></script>

    <script type="text/javascript" src="~/Content/Circloid/js/optional/jqvmap/jquery.vmap.min.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/jqvmap/maps/jquery.vmap.world.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/jqvmap/maps/continents/jquery.vmap.africa.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/jqvmap/maps/continents/jquery.vmap.asia.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/jqvmap/maps/continents/jquery.vmap.australia.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/jqvmap/maps/continents/jquery.vmap.europe.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/jqvmap/maps/continents/jquery.vmap.north-america.js"></script>
    <script type="text/javascript" src="~/Content/Circloid/js/optional/jqvmap/maps/continents/jquery.vmap.south-america.js"></script>
    

    <!-- add optional JS plugin files here -->
    @Scripts.Render("~/bundles/angular/js")
    <!-- REQUIRED: User Editable JS Files -->
    <script type="text/javascript" src="~/Content/Circloid/js/script.js"></script>
    <!-- add additional User Editable files here -->
    @Scripts.Render("~/bundles/custom/js")
    <!-- Demo JS Files -->
    @*<script type="text/javascript" src="~/Content/Circloid/js/demo-files/ui-charts.js"></script>*@

    @RenderSection("scripts", required: false)
</body>
</html>
